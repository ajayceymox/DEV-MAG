        <referenceContainer name="header">
            <block class="Zazmic\AmazonMCF\Block\Adminhtml\Index" name="amazonmcf-tabs" after="-" template="Zazmic_AmazonMCF::tab-block.phtml"/>
        </referenceContainer>
    </body>
</page>
